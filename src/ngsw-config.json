{
  "index": "index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": ["favicon.ico", "index.html", "*.bundle.css", "*.bundle.js", "*.chunk.js"]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["assets/**"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "dataGroup1",
      "urls": ["!resources"],
      "cacheConfig": {
        "maxSize": 50,
        "maxAge": "3d12h"
      }
    }
  ]
}
